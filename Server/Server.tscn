script = ExtResource( 2 )

[connection signal="seconds_changed" from="WaitTimer" to="." method="on_wait_seconds"]
[connection signal="timeout" from="WaitTimer" to="." method="on_wait_timeout"]
[connection signal="seconds_changed" from="DraftingTimer" to="." method="on_drafting_seconds"]
[connection signal="timeout" from="DraftingTimer" to="." method="on_drafting_timeout"]
